{
  "name": "monaco-es",
  "displayName": "Monaco ES (alpha)",
  "description": "Configurable Monaco Editor (ECMAScript) modules using Rollup",
  "author": "Saleh Abdel Motaal <saleh@grasppe.com>",
  "license": "MIT",
  "version": "0.0.4",
  "main": "sources/index.mjs",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "bin": {
    "serve-monaco-es": "lib/server.mjs",
    "build-monaco-es": "lib/build.mjs"
  },
  "files": [
    "config",
    "lib",
    "rollup.config.js",
    "sources/index.mjs",
    "sources/environment.mjs",
    "sources/monaco.mjs",
    "sources/worker.mjs",
    "sources/debugging.mjs",
    "sources/modules/features.mjs",
    "static",
    "types.d.ts",
    "test"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "node --experimental-modules ./lib/server.mjs",
    "install": "node --experimental-modules ./lib/build.mjs"
  },
  "dependencies": {
    "monaco-editor": "*",
    "rollup": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-re": "*",
    "rollup-pluginutils": "*",
    "vscode-languageserver-types": "*"
  },
  "optionalDependencies": {
    "rollup-plugin-sourcemaps": "*",
    "rollup-plugin-terser": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "express": "^4.16.3"
  },
  "engines": {
    "node": "^10.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
